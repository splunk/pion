		Name="VCLinkerTool"
		AdditionalLibraryDirectories="&quot;$(OutDir)&quot;;&quot;$(LOG4CXX_HOME)\bin\static&quot;"
		GenerateDebugInformation="true"
	/>
</VisualStudioPropertySheet>
